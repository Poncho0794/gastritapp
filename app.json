{
  "expo": {
    "name": "gastritapp",
    "slug": "gastritapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/gastritapp_icon.png",
    "scheme": "gastritapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.gastritapp.gastritapp",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/gastritapp_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/gastritapp_icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/gastritapp_logo.png",
          "imageWidth": 150,
          "resizeMode": "contain",
          "backgroundColor": "#d8f0e8",
          "dark": {
            "image": "./src/assets/images/gastritapp_logo.png",
            "backgroundColor": "#122b23"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "52de070a-632a-44e2-bc26-15095c0b76a7"
      }
    }
  }
}
